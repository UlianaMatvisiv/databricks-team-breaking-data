    {
      "name": "39d52b3a",
      "displayName": "overview_paris",
      "queryLines": [
        "SELECT *\r\n",
        "FROM workspace.airbnb_gold.city_overview\r\n",
        "WHERE city = 'Paris';\r\n",
        "\r\n"
      ],
      "catalog": "workspace",
      "schema": "airbnb_gold"
    },
    {
      "name": "e334eae3",
      "displayName": "month",
      "queryLines": [
        "SELECT\r\n",
        "  city,\r\n",
        "  date_trunc('month', scrape_dt) AS month,\r\n",
        "  ROUND(AVG(avg_price_per_night), 2) AS avg_price_per_night\r\n",
        "FROM workspace.airbnb_gold.city_daily_metrics\r\n",
        "WHERE city IN ('Paris', 'Venice')\r\n",
        "GROUP BY city, date_trunc('month', scrape_dt)\r\n",
        "ORDER BY month;"
      ],
      "catalog": "workspace",
      "schema": "airbnb_gold"
    },
    {
      "name": "072b8d4b",
      "displayName": "quarter",
      "queryLines": [
        "SELECT\r\n",
        "  city,\r\n",
        "  date_trunc('quarter', scrape_dt) AS quarter,\r\n",
        "  ROUND(AVG(avg_price_per_night), 2) AS avg_price\r\n",
        "FROM workspace.airbnb_gold.city_daily_metrics\r\n",
        "WHERE city IN ('Paris', 'Venice')\r\n",
        "GROUP BY city, date_trunc('quarter', scrape_dt)\r\n",
        "ORDER BY quarter;\r\n"
      ],
      "catalog": "workspace",
      "schema": "airbnb_gold"
    },
    {
      "name": "4983dff3",
      "displayName": "availability",
      "queryLines": [
        "SELECT\r\n",
        "  city,\r\n",
        "  date_trunc('month', scrape_dt) AS month,\r\n",
        "  ROUND(AVG(occupancy_rate_estimate), 3) AS avg_occupancy_rate\r\n",
        "FROM workspace.airbnb_gold.city_daily_metrics\r\n",
        "WHERE city IN ('Paris', 'Venice')\r\n",
        "GROUP BY city, date_trunc('month', scrape_dt)\r\n",
        "ORDER BY month;"
      ]
    },
    {
      "name": "5772a49e",
      "displayName": "roomtype",
      "queryLines": [
        "SELECT\r\n",
        "  city,\r\n",
        "  room_type,\r\n",
        "  COUNT(*) AS listings_count,\r\n",
        "  ROUND(\r\n",
        "    100 * COUNT(*) / SUM(COUNT(*)) OVER (PARTITION BY city), 2\r\n",
        "  ) AS pct_of_total\r\n",
        "FROM workspace.airbnb_silver.listings\r\n",
        "WHERE city == 'Venice'\r\n",
        "GROUP BY city, room_type\r\n",
        "ORDER BY city, pct_of_total DESC;"
      ],
      "catalog": "workspace",
      "schema": "airbnb_gold"
    },
    {
      "name": "a2daad07",
      "displayName": "crosscity",
      "queryLines": [
        "SELECT\r\n",
        "  city,\r\n",
        "  MAX(scrape_dt) AS last_date,\r\n",
        "  MAX_BY(total_listings, scrape_dt) AS total_listings,\r\n",
        "  ROUND(MAX_BY(avg_price_per_night, scrape_dt), 2) AS avg_price_per_night,\r\n",
        "  ROUND(MAX_BY(occupancy_rate_estimate, scrape_dt), 3) AS avg_occupancy_rate,\r\n",
        "  ROUND(MAX_BY(avg_review_score, scrape_dt), 2) AS avg_review_score\r\n",
        "FROM workspace.airbnb_gold.city_daily_metrics\r\n",
        "WHERE \r\n",
        "  (city = 'Paris'  AND MONTH(TO_DATE(scrape_dt)) = 6) OR\r\n",
        "  (city = 'Venice' AND MONTH(TO_DATE(scrape_dt)) = 9)\r\n",
        "GROUP BY city\r\n",
        "ORDER BY city;\r\n"
      ]
    },
    {
      "name": "be6d14b4",
      "displayName": "gold",
      "queryLines": [
        "SELECT *\r\n",
        "FROM workspace.airbnb_gold.city_daily_metrics;"
      ]
    },
    {
      "name": "a218c539",
      "displayName": "overview_venice",
      "queryLines": [
        "SELECT *\r\n",
        "FROM workspace.airbnb_gold.city_overview\r\n",
        "WHERE city = 'Venice';\r\n",
        "\r\n"
      ],
      "catalog": "workspace",
      "schema": "airbnb_gold"
    },
    {
      "name": "60cabc0a",
      "displayName": "neighborhood",
      "queryLines": [
        "SELECT\r\n",
        "  city,\r\n",
        "  neighbourhood_cleansed AS neighborhood,\r\n",
        "  ROUND(AVG(avg_price), 2) AS avg_price,\r\n",
        "  ROUND(AVG(occupancy_rate_estimate), 3) AS avg_occupancy_rate,\r\n",
        "  SUM(listings_count) AS listings_count\r\n",
        "FROM workspace.airbnb_gold.neighborhood_metrics\r\n",
        "WHERE city IN ('Paris', 'Venice')\r\n",
        "GROUP BY city, neighbourhood_cleansed\r\n",
        "ORDER BY city, avg_price DESC;\r\n"
      ],
      "catalog": "workspace",
      "schema": "airbnb_gold"
    },
    {
      "name": "cbbae50f",
      "displayName": "percentdiff",
      "queryLines": [
        "WITH latest AS (\r\n",
        "  SELECT\r\n",
        "    city,\r\n",
        "    MAX_BY(total_listings, scrape_dt)      AS total_listings,\r\n",
        "    ROUND(MAX_BY(avg_price_per_night, scrape_dt),2)  AS avg_price_per_night,\r\n",
        "    ROUND(MAX_BY(occupancy_rate_estimate, scrape_dt),3) AS avg_occupancy_rate,\r\n",
        "    ROUND(MAX_BY(avg_review_score, scrape_dt),2)    AS avg_review_score\r\n",
        "  FROM workspace.airbnb_gold.city_daily_metrics\r\n",
        "  WHERE city IN ('Paris','Venice')\r\n",
        "  GROUP BY city\r\n",
        ")\r\n",
        "SELECT\r\n",
        "  p.city AS city_a, v.city AS city_b,\r\n",
        "  ROUND( (v.avg_price_per_night - p.avg_price_per_night) / p.avg_price_per_night * 100, 2) AS pct_diff_price,\r\n",
        "  ROUND( (v.avg_occupancy_rate - p.avg_occupancy_rate) / NULLIF(p.avg_occupancy_rate,0) * 100, 2) AS pct_diff_occupancy,\r\n",
        "  ROUND( (v.total_listings - p.total_listings) / NULLIF(p.total_listings,0) * 100, 2) AS pct_diff_listings,\r\n",
        "  ROUND( (v.avg_review_score - p.avg_review_score) / NULLIF(p.avg_review_score,0) * 100, 2) AS pct_diff_rating\r\n",
        "FROM\r\n",
        "  (SELECT * FROM latest WHERE city = 'Paris') p\r\n",
        "CROSS JOIN\r\n",
        "  (SELECT * FROM latest WHERE city = 'Venice') v;\r\n"
      ]
    },
    {
      "name": "1d20316d",
      "displayName": "topneighborhoods",
      "queryLines": [
        "WITH latest_per_city AS (\r\n",
        "  SELECT\r\n",
        "    city,\r\n",
        "    MAX(scrape_dt) AS latest_dt\r\n",
        "  FROM workspace.airbnb_gold.neighborhood_metrics\r\n",
        "  WHERE city IN ('Paris', 'Venice')\r\n",
        "  GROUP BY city\r\n",
        "),\r\n",
        "filtered AS (\r\n",
        "  SELECT m.*\r\n",
        "  FROM workspace.airbnb_gold.neighborhood_metrics m\r\n",
        "  INNER JOIN latest_per_city l\r\n",
        "    ON m.city = l.city AND m.scrape_dt = l.latest_dt\r\n",
        "),\r\n",
        "ranked AS (\r\n",
        "  SELECT\r\n",
        "    city,\r\n",
        "    neighbourhood_cleansed AS neighborhood,\r\n",
        "    ROUND(avg_price, 2) AS avg_price,\r\n",
        "    ROUND(occupancy_rate_estimate, 3) AS occupancy_rate,\r\n",
        "    listings_count,\r\n",
        "    ROW_NUMBER() OVER (PARTITION BY city ORDER BY avg_price DESC) AS city_rank\r\n",
        "  FROM filtered\r\n",
        ")\r\n",
        "SELECT\r\n",
        "  city,\r\n",
        "  neighborhood,\r\n",
        "  avg_price,\r\n",
        "  occupancy_rate,\r\n",
        "  listings_count,\r\n",
        "  CASE\r\n",
        "    WHEN city = 'Paris' THEN city_rank\r\n",
        "    WHEN city = 'Venice' THEN city_rank + 10\r\n",
        "  END AS rnk\r\n",
        "FROM ranked\r\n",
        "WHERE city_rank <= 10\r\n",
        "ORDER BY rnk;\r\n"
      ],
      "catalog": "workspace",
      "schema": "airbnb_gold"
    }
  ],
